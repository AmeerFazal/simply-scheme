#lang racket
(require (planet dyoo/simply-scheme:2:2))

;; 3.2

(+ 3 (* 4 5) (- 10 4))     ; 3
(+ (* (- (/ 8 2) 1) 5) 2)  ; 4
(* (+ (- 3
         (sin
          (- 8
             (- (/ 2
                   4))
             (/ 4 2))
          (* 3 2))
         (sqrt 5)))
   3)                      ; 10